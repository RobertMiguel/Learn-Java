package com.programming.app.vetor;

import java.util.Scanner;

public class q03 {
  public static void main(String[] args) {
    
    Scanner input = new Scanner(System.in);

    int i, j = 0;
    int numero;
    int vetor01[] = new int[10];
    int vetor02[] = new int[10];
    int intercalacao[] = new int[20];

    for (i = 0; i < 10; i++) {
      System.out.println("Digite um nÃºmero para o vetor 1 :");
      vetor01[i] = input.nextInt(); 
      intercalacao[j] = vetor01[i];
      j++;
    }    

    j = 0;

    for (int k = 0; k < 20; k++) {
      System.out.println(intercalacao[i]);
    }

    input.close();

  }
}
